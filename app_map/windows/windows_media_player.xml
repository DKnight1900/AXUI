<?xml version="1.0"?>

<AXUI_windows:elements xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="AXUI_windows AXUI_windows.xsd">

    <AXUI_windows:include name="desktop" path="window_desktop.xml" />
    <AXUI_windows:include name="funcs" path="windows_media_player_funcs.xml" />
    
    <AXUI_windows:window ID="" name="library_window" parent="desktop.desktop" identifier="name='Windows Media Player'" start_cmd="C:\Program Files\Windows Media Player\wmplayer.exe">
        <AXUI_windows:menu_bar ID="" name="menu_bar" identifier="name='menu bar'" start_func="funcs.func1">
            <AXUI_windows:menu_item ID="" name="File" identifier="name='File'" />
            <AXUI_windows:menu_item ID="" name="View" identifier="name='View'" />
            <AXUI_windows:menu_item ID="" name="Play" identifier="name='Play'" />
            <AXUI_windows:menu_item ID="" name="Tools" identifier="name='Tools'" />
