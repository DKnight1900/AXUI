<?xml version="1.0"?>
<AXUI_funcs:funcs xmlns:AXUI_funcs="AXUI_funcs" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="AXUI_funcs AXUI_funcs.xsd">
    <AXUI_funcs:include_app_map name="wmplayer_app_map" path="windows_media_player.xml" />
    
    <AXUI_funcs:func name="func1" description="">
        <AXUI_funcs:step cmd="C:\Program Files\Windows Media Player\wmplayer"/>
    </AXUI_funcs:func>
    
    <AXUI_funcs:func name="func2" description="">
        <AXUI_funcs:step app_map="wmplayer_app_map" cmd="wmplayer.keyboard.input CTRL+M" />
    </AXUI_funcs:func>
    
    <AXUI_funcs:func name="func3" description="">
        <AXUI_funcs:step app_map="wmplayer_app_map" cmd="wmplayer.mouse.move" />
    </AXUI_funcs:func>
    
    <AXUI_funcs:func name="func4" description="">
        <AXUI_funcs:step app_map="wmplayer_app_map" cmd="wmplayer.WindowPattern.SetWindowVisualState "/>
    </AXUI_funcs:func>
    
</AXUI_funcs:funcs>
