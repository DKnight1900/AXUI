        </AXUI_windows:menu_bar>
        <AXUI_windows:group ID="" name="transport_button_group" start_func="funcs.func2">
            <AXUI_windows:button ID="" name="stop" identifier="name='stop'"/>
            <AXUI_windows:button ID="" name="previous" identifier="name='previous'"/>
            <AXUI_windows:button ID="" name="play" identifier="name='play'"/>
            <AXUI_windows:button ID="" name="next" identifier="name='next'"/>
